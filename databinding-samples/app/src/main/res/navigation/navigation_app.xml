<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_app"
    app:startDestination="@id/testFragment"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.android.databinding.basicsample.ui.testcase.TestFragment"
        android:label="TestFragment"
        tools:layout="@layout/fragment_test_fragment">

        <!-- Super Simple action to cause onDestroyView to be called -->
        <action
            android:id="@+id/action_testFragment_self"
            app:exitAnim="@android:anim/slide_out_right"
            app:destination="@id/testFragment" />
    </fragment>
</navigation>
